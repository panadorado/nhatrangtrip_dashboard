<img class="wallpaper" src="/images/nhatrang1.jpg" alt="">
<div class="container" id="container">
	<div class="form-container sign-in-container">
		<form method="POST" action="/login">
			<h1>Đăng Nhập</h1>
			<div class="logo">
				<img src="/assets/profile.jpg" alt="">
			</div>
			{{#if error}} 
				<span class="login-error">{{error.statusText}} - {{error.message}}</span>
			{{else}}
				<span class="login-none"></span>
			{{/if}}
			<input type="email" name="email" placeholder="Email" />
			<input type="password" name="password" placeholder="Password" />
			<a href="#">Forgot your password?</a>
			<input class="button" type="submit" value="Đăng Nhập">
		</form>
	</div>
	<div class="overlay-container">
		<div class="overlay">
			<div class="overlay-panel overlay-right">	
				<div class="Mainstage">Chào mừng đến với</div>
				<div class="stage">
					<div class="wrapper">
						<div class="slash"></div>
						<div class="sides">
							<div class="side"></div>
							<div class="side"></div>
							<div class="side"></div>
							<div class="side"></div>
						</div>
						<div class="text">
							<div class="text--backing">NhaTrangTrip</div>
							<div class="text--left">
							<div class="inner">NhaTrangTrip</div>
						</div>
						<div class="text--right">
							<div class="inner">NhaTrangTrip</div>
						</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
